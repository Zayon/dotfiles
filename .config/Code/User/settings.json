    "colorize.languages": [
        "css",
        "sass",
        "scss",
        "less",
        "postcss",
        "sss",
        "stylus",
        "xml",
        "svg",
        "i3",
        "ini"
    ],
    "editor.fontFamily": "'Jetbrains Mono'",
    "editor.fontSize": 16,
    "window.zoomLevel": 0.5,
    "workbench.colorTheme": "Solarized Light",
    "files.associations": {
        "**/.i3/config": "i3",
        ".i3/config": "i3"
    }
}